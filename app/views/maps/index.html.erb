<p id="notice"><%= notice %></p>

<h1>Maps</h1>

<table class=table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Startx</th>
      <th>Starty</th>
      <th>Endx</th>
      <th>Endy</th>
      <th>Height</th>
      <th>Width</th>
      <th>Min activeness</th>
      <th>Max activeness</th>
      <th>Min prosperity</th>
      <th>Max prosperity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @maps.each do |map| %>
      <tr>
        <td><%= link_to map.name, map %></td>
        <td><%= map.startx %></td>
        <td><%= map.starty %></td>
        <td><%= map.endx %></td>
        <td><%= map.endy %></td>
        <td><%= map.height %></td>
        <td><%= map.width %></td>
        <td><%= map.min_activeness %></td>
        <td><%= map.max_activeness %></td>
        <td><%= map.min_prosperity %></td>
        <td><%= map.max_prosperity %></td>
        <td><%= link_to 'Show', map %></td>
        <td><%= link_to 'Edit', edit_map_path(map) %></td>
        <td><%= link_to 'Destroy', map, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Map', new_map_path %>
